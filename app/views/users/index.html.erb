<%= provide(:title, "All users") %>
<%= render 'shared/title', title: "All Users" %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Created at</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <%= render @users %>
  </tbody>
</table>
<%= will_paginate %>
